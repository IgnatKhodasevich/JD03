<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.carservice.entities.Generations" table="generations" schema="cars">
        <property name="generationId" column="generation_id"/>
        <property name="generationName" column="generation_name"/>
        <property name="modelId" column="model_id"/>
        <property name="yearBegin" column="year_begin"/>
        <property name="yearEnd" column="year_end"/>
    </class>
</hibernate-mapping>